source 'https://github.com/CocoaPods/Specs.git'
source 'https://github.com/aliyun/aliyun-specs.git'

project 'WTKNeiHanJoke/WTKNeiHanJoke.xcodeproj'

platform :ios, '9.0'

target 'WTKNeiHanJoke' do
  use_frameworks!
  inhibit_all_warnings! #压制所有警告
  
  pod 'Reveal-SDK', '~> 4', :configurations => ['Debug'] # 开发调试界面
  pod 'SwiftyJSON'
  pod 'SnapKit', '~> 4.0.0'
  pod 'TZImagePickerController' # 图片选择器
  pod 'IQKeyboardManagerSwift', '~> 5.0.6'  # 键盘处理
  #pod 'MJRefresh'   # 刷新
  pod 'MJRefresh', '~> 3.1.13'
  pod 'Toast-Swift', '~> 3.0.0' # 吐司
  pod 'MJExtension' # model转换
  pod 'Kingfisher', '~> 4.3.0'
  pod 'NJKWebViewProgress' # 网页进度条
  pod 'Toucan', '~> 0.6.0' # 图片裁剪
  pod 'GDPerformanceView-Swift', '~> 1.2.0' # 性能监控
  pod 'SDCycleScrollView','~> 1.64' # 轮播图
  pod 'FDFullscreenPopGesture' # 全屏侧滑返回
  pod 'KVOController' # facebook kvo处理类库
  pod 'Bugly'  # 和下面的只能用一个否则会报错
  pod 'Alamofire', '~> 4.4' # 网络请求
  pod 'AlamofireNetworkActivityIndicator' # 状态栏请求标识
  pod 'RSKGrowingTextView', '~> 3.0.0' # textView扩展
  pod 'MBProgressHUD'
  pod 'JGProgressHUD'
  pod 'JXPhotoBrowser', '~> 0.4.2' # 图片查看器
  # Rx
  pod 'RxSwift', '~> 4.0.0'
  pod 'RxCocoa', '~> 4.0.0'
  
  
  # U-Share SDK UI模块（分享面板，建议添加）
  pod 'UMengUShare/UI'
  # 集成新浪微博
  pod 'UMengUShare/Social/Sina'
  # 集成微信
  pod 'UMengUShare/Social/WeChat'
  # 集成QQ
  pod 'UMengUShare/Social/QQ'
  # 友盟统计
  pod 'UMengAnalytics'


end
